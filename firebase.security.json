{
  "rules": {
    "users": {
      "$uid": {
        ".read": "null !== auth && $uid === auth.uid"
      },
      ".write": true
    },
    "projects": {
      ".read": true,
      ".write": "null !== auth",
      "$projectId": {
        ".write": "newData.child('uid').val() === auth.uid"
      }
    }
  }
}
